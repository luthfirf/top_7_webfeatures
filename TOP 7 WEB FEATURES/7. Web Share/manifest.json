// Share Target API

{
    "name": "Fireship",
    "share_target": {
        "action": "/share-photo",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "title": "name",
            "text": "description",
            "url": "link",
            "files": [{
                "name": "photos",
                "accept": "image/png"
            }]
        }
    }
}